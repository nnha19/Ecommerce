pon={appliedCoupon}\r\n            fetchData={(url, method, data) =>\r\n              fetchData(url, method, data, context.token)\r\n            }\r\n          />\r\n        )}\r\n        {error && <p className=\"coupon-error\">{error}</p>}\r\n        {appliedCoupon && (\r\n          <p className=\"applied-coupon\">\r\n            <span>\r\n              <span className=\"coupon-code\">{appliedCoupon.code}</span> Applied\r\n            </span>\r\n            <span>\r\n              <span className=\"discount-price\">\r\n                {appliedCoupon.discountPrice}KS\r\n              </span>\r\n              &nbsp; off\r\n            </span>\r\n            <span\r\n              onClick={removeAppliedCouponHandler}\r\n              className=\"delete-coupon\"\r\n            >\r\n              x\r\n            </span>\r\n          </p>\r\n        )}\r\n        <li className=\"total\">\r\n          <div className=\"total-price-container\">\r\n            <span className=\"order-summary__text\">Total</span>\r\n            <span\r\n              className={`${\r\n                appliedCoupon ? \"original-price\" : \"\"\r\n              } order-summary__ks`}\r\n            >\r\n              {maniStr(allTotalAmount)} KS\r\n            </span>\r\n          </div>\r\n          {discountTotal && (\r\n            <p className=\"discount-price\">{maniStr(discountTotal)} KS</p>\r\n          )}\r\n        </li>\r\n      </ul>\r\n      <SecondaryBtn\r\n        disabled={props.disabled}\r\n        clicked={props.clicked || clickedBtnHandler}\r\n      >\r\n        {props.action}\r\n      </SecondaryBtn>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default OrderSummary;\r\n","C:\\Users\\User\\Desktop\\Portfolio\\sunglasses\\client\\src\\cart\\components\\Checkout\\CheckoutForm\\CheckoutForm.js",[],"C:\\Users\\User\\Desktop\\Portfolio\\sunglasses\\client\\src\\cart\\components\\Cart\\RemoveItemFromCart\\RemoveItemFromCart.js",["378","379","380","381","382"],"import React, { useState, use