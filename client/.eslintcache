er\"\r\n              clicked={() => placeOrderHandler()}\r\n            />\r\n          </div>\r\n        </div>\r\n        <Cart className=\"checkout__item-summary\" checkout={true} />\r\n      </div>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default Checkout;\r\n","C:\\Users\\User\\Desktop\\Portfolio\\sunglasses\\client\\src\\share\\components\\FormInput\\FormInput.js",["356","357","358"],"import React, { useState, useReducer, useEffect } from \"react\";\r\n\r\nimport \"./FormInput.css\";\r\n\r\nconst checkValidity = (val, valType) => {\r\n  if (!valType) {\r\n    return true;\r\n  } else if (valType.type === \"REQUIRE\") {\r\n    return val.length > 0;\r\n  } else if (valType.type === \"MIN_LENGTH\") {\r\n    return val.length >= valType.amount;\r\n  }\r\n};\r\n\r\nconst inputReducer = (state, action) => {\r\n  switch (action.type) {\r\n    case \"change\":\r\n      return {\r\n        ...state,\r\n        value: action.value,\r\n        valid: action.checkValidity(),\r\n        isTouched: true,\r\n      };\r\n  }\r\n};\r\n\r\nconst FormInput = (props) => {\r\n  const [inputVal, dispatch] = useReducer(inputReducer, {\r\n    value: props.value ? props.value : \"\",\r\n    valid: props.id === \"message\" && true,\r\n    isTouched: false,\r\n  });\r\n\r\n  useEffect(() => {\r\n    props.changeLoginVal(inputVal, props.id);\r\n  }, [inputVal.value]);\r\n\r\n  const inputChangeHandler = (e) => {\r\n    const value = e.target.value;\r\n    dispatch({\r\n      type: \"change\",\r\n      value,\r\n      checkValidity: () => checkValidity(value, props.validRules),\r\n    });\r\n  };\r\n\r\n  if (props.cleanInput) {\r\n    inputVal.value = \"\";\r\n  }\r\n\r\n  let output;\r\n  if (props.elementType === \"input\") {\r\n    output = (\r\n      <div className={`form__input-container ${props.containerCls}`}>\r\n        {props.label && (\r\n          <label className={`form__label ${props.labelCls}`}>\r\n            {props.label}\r\n          </label>\r\n        )}\r\n        <input\r\n          disabled={props.disabled}\r\n          placeholder={props.placeholder}\r\n          value={inputVal.value}\r\n          onChange={inputChangeHandler}\r\n          className={`form__input ${props.inputCls}`}\r\n          type={props.type}\r\n        />\r\n        {!inputVal.valid && inputVal.isTouched && (\r\n          <p className=\"form__err-msg\">{props.errorMsg}</p>\r\n        )}\r\n      </div>\r\n    );\r\n  } else if (props.elementType === \"textarea\") {\r\n    output = (\r\n      <div className={`form__input-container ${props.containerCls}`}>\r\n        {props.label && (\r\n          <label className={`form__label ${props.labelCls}`}>\r\n            {props.label}\r\n          </label>\r\n        )}\r\n        <textarea\r\n          disabled={props.disabled}\r\n          value={inputVal.value}\r\n          onChange={inputChangeHandler}\r\n          className={`form__input ${props.inputCls}`}\r\n          rows={props.rows || \"4\"}\r\n          cols={props.cols || \"50\"}\r\n        ></textarea>\r\n        {!inputVal.valid && inputVal.isTouched && (\r\n          <p className=\"form__err-msg\">{props.errorMsg}</p>\r\n        )}\r\n      </div>\r\n    );\r\n  }\r\n  return output ? output : null;\r\n};\r\n\r\nexport default FormInput;\r\n","C:\\Users\\User\\Desktop\\Portfolio\\sunglasses\\client\\src\\cart\\components\\Cart\\Cart.js",["359","360"],"import React, { useContext } from \"react\";\r\nimport { useHistory } from \"react-router-dom\";\r\n\r\nimport ProductQuantity from \"../../../products/components/ProductDetail/ProductDetailBody/ProductQuantity/ProductQuantity\";\r\nimport RemoveItemFromCart from \"./RemoveItemFromCart/RemoveItemFromCart\";\r\nimport OrderSummary from \"./OrderSummary/OrderSummary\";\r\nimport AddToWhilist from \"./AddToWhilist/AddToWhilist\";\r\nimport Context from \"../../../contexts/context\";\r\nimport SecondaryBtn from \"../../../share/components/SecondaryBtn/SecondaryBtn\";\r\n\r\nimport \"./Cart.css\";\r\n\r\nc